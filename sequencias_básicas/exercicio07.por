programa {
  funcao inicio() {

    real numero, dobro, tercaParte

    escreva("Digite um número: ")
    leia(numero)

    dobro = numero * 2
    tercaParte = numero / 3

    escreva("O dobro de ", numero, " é ", dobro, "\n")
    escreva("A terça parte de ", numero, " é ", tercaParte, "\n")
    
  }
}
